{
  "name": "wye-support-service",
  "version": "1.0.0",
  "description": "Captures and stores steam game data, and provides endpoints for access to data for Wye",
  "repository": "https://github.com/n-bryant/wye-support-service.git",
  "license": "MIT",
  "scripts": {
    "start": "nodemon -e js src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy": "prisma deploy --env-file variables.env",
    "dev": "nodemon -e js --inspect src/server.js"
  },
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "6.0.0",
    "express": "^4.17.1",
    "lodash.get": "^4.4.2",
    "moment": "^2.24.0",
    "node-cron": "^2.0.3",
    "nodemon": "^1.18.7",
    "prisma": "1.32.0-beta",
    "supertest": "^4.0.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "devDependencies": {
    "jest": "^24.9.0"
  }
}
